{
  "version": 2,
  "builds": [
    { "src": "docs/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/docs/index.html" },
    { "src": "/punchlist", "dest": "/docs/punchlist.html" },
    { "src": "/schedule", "dest": "/docs/schedule.html" },
    { "src": "/(.*)", "dest": "/docs/$1" }
  ]
}

